{
  "name": "adonis-hexa",
  "version": "3.2.3",
  "description": "A cli tool for adonis hexa.",
  "main": "index.js",
  "bin": "dist/index.js",
  "repository": "https://github.com/ogbiyoyosky/adonis-hexa-cli",
  "scripts": {
    "test": "node dist/test/generatorTest.spec.js",
    "babel-node": "babel-node --presets=es2015",
    "build": "rimraf dist/ && babel app --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "dev-server": "nodemon --exec npm run babel-node && standard--fix -- app/index.js ",
    "start": " npm run build && node dist/index.js",
    "lint": "standard --fix"
  },
  "author": "Emmanuel ogbiyoyo",
  "license": "GPL-3.0-only",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "japa": "^2.0.5",
    "nodemon": "^1.18.4",
    "rimraf": "^2.6.2",
    "standard": "^12.0.1"
  },
  "dependencies": {
    "chai": "^4.2.0",
    "cli-spinner": "^0.2.8",
    "commander": "^2.19.0",
    "cross-spawn": "^6.0.5",
    "eslint-plugin-import": "^2.14.0",
    "figlet": "^1.2.1",
    "fs-extra": "^7.0.0",
    "kleur": "^2.0.2",
    "minimist": "^1.2.0",
    "mocha": "^5.2.0",
    "mustache": "^3.0.0",
    "nodegit": "^0.22.2",
    "shelljs": "^0.8.2"
  }
}
